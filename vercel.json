{
  "version": 2,
  "installCommand": "npm ci",
  "buildCommand": "bash -lc 'cd packages/admin-web && npm run build'",
  "framework": null,
  "outputDirectory": "packages/admin-web/dist",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  }
}
